// program asks the user to enter two intergers and compare them
#include <iostream>

using std::cout;
using std::cin;

//function main begins

int main()
{
	int number1; //variable declaration
	int number2; //varible declaration

	cout << "Please enter two integer values: ";
	cin >> number1 >> number2;

	if (number1 > number2)
		cout << number1 <<" is larger! ";
	if (number1 < number2)
		cout << number2 << " is larger! ";
	if (number1 == number2)
		cout << "These numbers are equal! ";

}//end function main
